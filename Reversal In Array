public class ReversalInArray {
    public static void main(String[] args) {
        int [] arr = {2,4,8,10,12,16};
        int i=0;
        int j=arr.length-1;
        while(i<j){
            int temp= arr[i];
            arr[i] = arr[j];
            arr[j]=temp;
            i++;
            j--;
        }
        for(int p:arr){
            System.out.println(p);
        }
    }
}
